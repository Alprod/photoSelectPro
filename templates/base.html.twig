<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}PhotoSelect Pro{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {{ encore_entry_script_tags('glb') }}
        {% endblock %}
    </head>
    <body>
    <div class="bg-white">
        <div class="relative overflow-hidden">
            {% block alert %}
                {% include 'includes/alert/_alert.html.twig' %}
            {% endblock %}
            {% block header %}
                {% include 'includes/header/_container_header.html.twig' %}
            {% endblock %}
            
            
            <main>
                {% block body %}{% endblock %}
            </main>
            {% block footer %}
                {% include 'includes/footer/_footer.html.twig' %}
            {% endblock %}
        </div>
    </div>
    </body>
</html>
